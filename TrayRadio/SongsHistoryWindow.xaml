<Window x:Class="TrayRadio.SongsHistoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:TrayRadio"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:prop="clr-namespace:TrayRadio.Properties"
        Title="Songs History"
        Width="{Binding Source={x:Static prop:Settings.Default}, Path=SongsHistoryWindowWidth}"
        Height="{Binding Source={x:Static prop:Settings.Default}, Path=SongsHistoryWindowHeight}"
        Closing="Window_Closing"
        Loaded="Window_Loaded"
        ResizeMode="CanResize"
        WindowStartupLocation="CenterScreen"
        mc:Ignorable="d">
	<Grid Margin="5">
		<TextBox Name="txbHistory" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto"/>
	</Grid>
</Window>
